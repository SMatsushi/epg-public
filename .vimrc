"# See also: http://qiita.com/tetsuya/items/75987487ffac42250d3b
"   Color scheme:
"     http://nanasi.jp/articles/colorscheme/default_install/elflord/view.html
"	  http://www.yuichon.com/2013/04/vim-colorscheme/
"   vimrc:
"     http://espion.just-size.jp/archives/06/034090759.html
"   
""# set number

set title
syntax on
"# set showmatch
set noshowmatch
set ts=4
set sw=4
set expandtab

" colorscheme desert
colorscheme elflord

"  Language
set fileencodings=utf-8,iso-2022-jp,euc-jp,cp932
set termencoding=utf-8
set fenc=utf-8
set enc=utf-8
" Search
set ignorecase
set incsearch
"set smartindent
"set smartcase
set wrapscan
set scrolloff=5
set history=50


